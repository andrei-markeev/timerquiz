{
    "version": 2,
    "rewrites": [
        { "source": "/", "destination": "/api/quiz/index" },
        { "source": "/join", "destination": "/api/quiz/join" },
        { "source": "/create", "destination": "/api/quiz/create" },
        { "source": "/edit", "destination": "/api/quiz/edit" },
        { "source": "/delete", "destination": "/api/quiz/delete" },
        { "source": "/manage", "destination": "/api/quiz/manage" },
        { "source": "/present", "destination": "/api/quiz/present" },
        { "source": "/play", "destination": "/api/quiz/play" },
        { "source": "/answer", "destination": "/api/quiz/answer" },
        { "source": "/login", "destination": "/api/user/login" },
        { "source": "/logout", "destination": "/api/user/logout" },
        { "source": "/api/user/google", "destination": "/api/user/google" }
    ],
    "regions": ["arn1"],
    "env": {
        "MONGO_URL": "@quiz-mongo-url",
        "GOOGLE_APP_ID": "1023788359179-pueabr7n69fqs55sj8nfkg9ssvj3j54p.apps.googleusercontent.com",
        "GOOGLE_APP_SECRET": "@quiz-google-app-secret"
    }
}